{
	"version": 2,
	"buildCommand": "yarn run build",
	"devCommand": "yarn run dev",
	"installCommand": "yarn install",
	"outputDirectory": "dist",
	"builds": [
		{
			"src": "src/index.ts",
			"use": "@vercel/node",
			"config": { "includeFiles": ["dist/**"] }
		}
	],
	"routes": [
		{
			"src": "/(.*)",
			"dest": "/src/index.ts"
		}
	]
}
